---
# UNTRUSTED INTERFACE CONFIG
# Untrusted/outside interface
untrusted_if_name: "eth1"
# IP address of untrusted/outside interface
untrusted_if_ip: "192.168.0.150/20"
# Default gateway of untrusted/outside interface
untrusted_if_gw: "192.168.0.1"

# VPN CONFIG
# IKE local identifier for VPNs
local_id: "192.168.0.150"
# Uplink VPNs
uplinks:
  0:
    remote_peer_ip: 192.168.0.252
    # Optional: If not set, defaults to remote_peer_ip
    # remote_id: 192.168.0.252
    # Optional: IPv6 prefix for tunnel/BGP peering between this host and the client VPN host.
    # prefix_uplink_tunnel: "fd33:2:f:1::/127"
    psk: secretpassword
  1:
    remote_peer_ip: 192.168.0.253
    # Optional: If not set, defaults to remote_peer_ip
    # remote_id: 192.168.0.253
    # Optional: IPv6 prefix for tunnel/BGP peering between this host and the client VPN host.
    # prefix_uplink_tunnel: "fd33:2:f:2::/127"
    psk: secretpassword

# OVERLAY CONFIG
# IPv6 prefix for client initiating administration traffic.
prefix_uplink: "fd33::/16"
# Tunnel transit prefix for link between trusted namespace and root namespace, must be a /127.
prefix_root_tunnel: "fd33:2:f::/127"
# IP prefix for customers. Must be a /16, will get subnetted into /24s per customer tunnel.
prefix_customer_v4: "100.99.0.0/16"
# IPv6 prefix for customers. Must be a /48. Will be subnetted into /96s per customer per tunnel.
prefix_customer_v6: "fdcc:0:c::/48"

## BGP config
# ASN must be between 4.200.000.000 and 4.294.967.294 inclusive.
bgp:
  asn: 4255786768
  router_id: "0.0.0.1"
...
