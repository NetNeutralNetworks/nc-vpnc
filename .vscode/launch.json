    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "sudo": true,
        },
        {
            "name": "Python: File - hub",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/vpnc",
            "args": [
                "hub"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "sudo": true,
        },
        {
            "name": "Python: Module - main",
            "type": "python",
            "request": "launch",
            "module": "vpnc.main",
            "console": "integratedTerminal",
            "justMyCode": true,
            "sudo": true,
            "cwd": "${workspaceFolder}/vpnc/"
        },
        {
            "name": "Python: Module - hub",
            "type": "python",
            "request": "launch",
            "module": "vpnc.vpnc_hub",
            "console": "integratedTerminal",
            "justMyCode": true,
            "sudo": true,
            "cwd": "${workspaceFolder}/vpnc/"
        },
        {
            "name": "Python: Module - endpoint",
            "type": "python",
            "request": "launch",
            "module": "vpnc.vpnc_endpoint",
            "console": "integratedTerminal",
            "justMyCode": true,
            "sudo": true,
            "cwd": "${workspaceFolder}/vpnc/"
        },
        {
            "name": "Python: Module - vpnctl",
            "type": "python",
            "request": "launch",
            "module": "vpnc.ctl.main",
            "console": "integratedTerminal",
            "justMyCode": true,
            "sudo": true,
            "cwd": "${workspaceFolder}/vpnc/",
            "args": [
                // "remote",
                // "D0001",
                // "commit",
                // "--dry-run",
                // "--diff",
                // "--revert"
                "service",
                "set",
                "--untrusted-if-name",
                "ens3",
                "--bgp-asn",
                "1234567",
            ]
        }
    ]
}
